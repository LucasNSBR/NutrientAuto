{
  "ConnectionStrings": {
    "SqlServerMain": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=NutrientDb;Integrated Security=True"
  },
  "MassTransit": {
    "RabbitMqHost": "rabbitmq://localhost",
    "RabbitMqHostUser": "guest",
    "RabbitMqHostPassword": "guest",
    "RabbitMqQueueName": "nutrient_queue"
  },
  "JwtToken": {
    "Issuer": "Nutrient LLC",
    "Audience": "nutrient.io",
    "ExpiresInSeconds": 2400
  },
  "SwaggerEndpoint": "/swagger/v1/swagger.json",
  "LogstashHost": "http://localhost:8080",
  "HealthCheckEndpoint": "/hc",
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Trace",
        "System": "Trace"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
